<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/K_05173833_gray"
    android:orientation="vertical">

    <com.lambui.healthcare_doctor.widget.viewInApp.SettingContentChild1
        android:id="@+id/stProfile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_45"
        app:contentSettingChild="@string/text_header_profile"
        app:typeSettingChild="header" />

    <com.lambui.healthcare_doctor.widget.viewInApp.SettingContentChild1
        android:id="@+id/stHistoryPatient"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contentSettingChild="@string/text_header_history"
        app:typeSettingChild="center" />
</LinearLayout>