<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.lambui.healthcare_doctor.widget.viewInApp.TopOptionCategories
        android:id="@+id/btnCreateSchedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:TopOptionCategoryHeader="@string/text_title_create_schedule"
        app:TopOptionCategoryIcon="@drawable/ic_categories_dentist" />

    <com.lambui.healthcare_doctor.widget.viewInApp.TopOptionCategories
        android:id="@+id/btnCreateExamination"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:TopOptionCategoryHeader="@string/text_title_create_examination"
        app:TopOptionCategoryIcon="@drawable/ic_categories_cardiology" />
</LinearLayout>